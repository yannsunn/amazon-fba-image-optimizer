{
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/api/process/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "env": {
    "MAX_FILE_SIZE_MB": "10",
    "MAX_CONCURRENT_UPLOADS": "8"
  }
}