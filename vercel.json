{
  "buildCommand": "cd amazon-fba-image-optimizer/frontend && npm run build",
  "outputDirectory": "amazon-fba-image-optimizer/frontend/.next",
  "installCommand": "cd amazon-fba-image-optimizer/frontend && npm install",
  "framework": "nextjs",
  "regions": ["hnd1"],
  "functions": {
    "amazon-fba-image-optimizer/frontend/app/api/*/route.ts": {
      "maxDuration": 30
    },
    "amazon-fba-image-optimizer/frontend/app/api/*/*/route.ts": {
      "maxDuration": 30
    },
    "amazon-fba-image-optimizer/frontend/app/api/*/*/*/route.ts": {
      "maxDuration": 30
    }
  }
}